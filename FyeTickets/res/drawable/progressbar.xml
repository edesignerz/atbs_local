<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
 
    <item android:id="@android:id/background">
        <shape android:shape="rectangle" >
            <corners android:radius="5dp" />
 
            <gradient
                android:angle="270"
                android:endColor="@color/event_filter_radius_seekbar_light_gray_header_color"
                android:startColor="@color/event_filter_radius_seekbar_light_gray_header_color" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape android:shape="rectangle" >
                <corners android:radius="5dp" />
 
                <gradient
                    android:angle="270"
                    android:endColor="#53C3EB"
                    android:startColor="#53C3EB" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle" >
                <corners android:radius="5dp" />
 
                <gradient
                    android:angle="270"
                    android:endColor="#53C3EB"
                    android:startColor="#53C3EB" />
            </shape>
        </clip>
    </item>
 
</layer-list>